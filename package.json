{
    "name": "shopify-app-node",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "set BUILD_PATH=./dist&& react-scripts build",
        "eject": "react-scripts eject",
        "local": "set NODE_ENV=local&& nodemon server/index.js --watch ./server",
        "start": "set NODE_ENV=local&& nodemon server/index.js --watch ./server",
        "production": "set NODE_ENV=production&& pm2 start server/index.js",
        "start:client": "react-scripts start"
    },
    "type": "module",
    "engines": {
        "node": ">=16.13.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iserioton/shopify-node-react-app.git"
    },
    "keywords": [
        "common-utils-functions"
    ],
    "author": "@iserioton",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/iserioton/shopify-node-react-app/issues"
    },
    "homepage": "https://github.com/iserioton/shopify-node-react-app#readme",
    "dependencies": {
        "@apollo/client": "3.6.6",
        "@shopify/app-bridge": "3.0.0",
        "@shopify/app-bridge-react": "3.0.0",
        "@shopify/app-bridge-utils": "3.0.0",
        "@shopify/polaris": "9.12.1",
        "@shopify/shopify-api": "3.1.0",
        "common-utils": "file:common-utils",
        "cookie-parser": "1.4.6",
        "dotenv": "16.0.1",
        "express": "4.18.1",
        "fs": "^0.0.1-security",
        "http-call": "file:http-call",
        "jsonwebtoken": "^8.5.1",
        "mysql": "^2.18.1",
        "nodemon": "^2.0.16",
        "pm2": "^5.2.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "web-vitals": "2.1.4"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
